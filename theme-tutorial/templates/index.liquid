<h2>{{ collections.frontpage.title }}</h2>
{% comment %}
  {% for product in collections.frontpage.products %}
    {% render 'product-item', product: product %}
  {% endfor %}
{% endcomment %}

<div class="wrapper">
  {%- assign channel = 'Coding with Robby' -%}

  {%- assign price = 998 -%}
  <h2>{{ channel }}</h2>
  <p>{{ price | money }}</p>

  {% comment %} <img src="{{ 'puppy.jpeg' | asset_img_url: 'medium' }}" alt=""> {% endcomment %}
</div>
